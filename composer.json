{
    "name": "ferotres/redsys-bundle",
    "description": "The FerotresRedsysBundle adds support for manage payments in redsys platform for Symfony4.",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Toni Fernandez",
            "email": "toni.fernandez@gmx.com"
        }
    ],
    "autoload": {
